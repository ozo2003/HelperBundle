{
    "name": "sludio/helper-bundle",
    "description": "Helper Bundle",
    "type": "symfony-bundle",
    "authors": [{
        "name": "sludio",
        "email": "davis.zalitis@slud.io"
    }],
    "license": [
        "MIT"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ozo2003/oci8.git"
        }
    ],
    "require" : {
        "php": ">=5.5.9",
        "symfony/symfony": "^2.8|^3.0",
        "snc/redis-bundle": "dev-master",
        "predis/predis": "^1.1",
        "gedmo/doctrine-extensions": "^2",
        "twig/twig": "^1",
        "league/oauth2-client": "^1.0|^2.0"
    },
    "require-dev": {
        "symfony/security-guard": "^2.8|^3.0"
    },
    "autoload" : {
        "psr-0" : {
            "Sludio\\HelperBundle\\" : ""
        }
    },
    "target-dir": "Sludio/HelperBundle",
    "branch-alias": {
        "dev-master": "master"
    }
}
