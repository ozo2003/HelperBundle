{
    "name": "sludio/helper-bundle",
    "description": "Helper Bundle for Symfony",
    "keywords": [
        "alert",
        "captcha",
        "flash",
        "helper",
        "lexik",
        "logger",
        "message",
        "mobiledetect",
        "notification",
        "oauth",
        "openid",
        "openidconnect",
        "pagination",
        "scripts",
        "sludio",
        "sonata",
        "sortable",
        "symfony",
        "translatable",
        "translation",
        "sitemap",
        "specification"
    ],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Dāvis Zālītis",
            "email": "davis@source.lv"
        }
    ],
    "homepage": "https://github.com/ozo2003/HelperBundle",
    "support": {
        "issues": "https://github.com/ozo2003/HelperBundle/issues"
    },
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "doctrine/dbal": "*",
        "doctrine/common": "*",
        "doctrine/orm": "*",
        "predis/predis": "*",
        "snc/redis-bundle": "*",
        "symfony/symfony": "*",
        "twig/twig": "*",
        "ircmaxell/random-lib": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sludio\\HelperBundle\\": ""
        }
    },
    "suggest": {
        "abraham/twitteroauth": "Needed to use Oauth extension",
        "guzzlehttp/guzzle:~6.0": "Needed to use Guzzle extension",
        "jms/i18n-routing-bundle": "Needed to use Translatable extension",
        "lcobucci/jwt": "Needed to use Openidconnect extension",
        "league/oauth1-client": "Needed to use Oauth extension",
        "league/oauth2-client": "Needed to use Oauth or Openidconnect extension",
        "lexik/translation-bundle": "Needed to use Lexik or Translatable extension",
        "mobiledetect/mobiledetectlib": "Needed to use Mobile extension",
        "psr/cache:^1.0": "Needed to use Guzzle extension",
        "psr/http-message": "Needed to use Oauth or Openidconnect or Script extension",
        "psr/log": "Needed to use Script extension",
        "sonata-project/admin-bundle": "Needed to use Lexik or Translatable extension",
        "sonata-project/doctrine-orm-admin-bundle": "Needed to use Lexik or Translatable extension",
        "symfony/filesystem": "Needed to use Guzzle extension",
        "symfony/stopwatch": "Needed to use Guzzle extension"
    }
}
